extend ../_template

block title
	title Фиолетовый гламур
block prepend headLinks
	link(href='https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css', rel='stylesheet')
block content
	+breadcrumbPortfolio('Фиолетовый гламур')
	.content
		.row.portfolio-preview
			+portfolioItem('../../img/portfolio/main/fioletoviy-glamur/1.jpg', '../../img/portfolio/mini/fioletoviy-glamur/1.jpg')
			+portfolioItem('../../img/portfolio/main/fioletoviy-glamur/2.jpg', '../../img/portfolio/mini/fioletoviy-glamur/2.jpg')
			+portfolioItem('../../img/portfolio/main/fioletoviy-glamur/3.jpg', '../../img/portfolio/mini/fioletoviy-glamur/3.jpg')
			+portfolioItem('../../img/portfolio/main/fioletoviy-glamur/4.jpg', '../../img/portfolio/mini/fioletoviy-glamur/4.jpg')
			+portfolioItem('../../img/portfolio/main/fioletoviy-glamur/5.jpg', '../../img/portfolio/mini/fioletoviy-glamur/5.jpg')
			+portfolioItem('../../img/portfolio/main/fioletoviy-glamur/6.jpg', '../../img/portfolio/mini/fioletoviy-glamur/6.jpg')
			+portfolioItem('../../img/portfolio/main/fioletoviy-glamur/7.jpg', '../../img/portfolio/mini/fioletoviy-glamur/7.jpg')
